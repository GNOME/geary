msgctxt "Default full date"
msgid "%B %-e, %Y %-l:%M %P"
msgstr "%B %-e, %Y %-l:%M %P"

#. / Datetime format for 24-hour time, i.e. 16:35
#. / See http://developer.gnome.org/glib/2.32/glib-GDateTime.html#g-date-time-format
#: ../../src/client/util/util-date.vala:71
